package GitHub9;

public class Class5 {

	public static void main(String[] args) {
		

		for (int i = 1; i <=10; i++) {
			System.out.print("This is the running of this while loop's ");
			System.out.print(i);
			if (i == 1)
				System.out.println("st time.");
			else if (i== 2)
				System.out.println("nd time.");
			else if (i == 3)
				System.out.println("rd time.");
			else
				System.out.println("th time.");
		}	
	}

}
