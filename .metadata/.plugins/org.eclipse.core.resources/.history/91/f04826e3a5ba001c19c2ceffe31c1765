package homeWorkDone;

	public class HomeWorkMain {
		
		static float subOneMonthlyCost = 14;
		static float subTwoMonthlyCost = 14;
		static float subThreeMonthlyCost = 10;
		static float subFourMonthlyCost = 14;
		
		public static void main(String[] Arg) {
			
			// Monthly subscription list :
			
			String subOne = "NetFlix";
			String subTwo = "Audible";
			String subThree = "Spotify";
			String subFour = "World of WarCraft";
			
			float dailySubCost = DailyCosts.subOneDailyCost + DailyCosts.subTwoDailyCost + DailyCosts.subThreeDailyCost + DailyCosts.subFourDailyCost;
			
//			float subOneMonthlyCost = 14;
//			float subTwoMonthlyCost = 14;
//			float subThreeMonthlyCost = 10;
//			float subFourMonthlyCost = 14;
			
			//Daily Cost list :
			
			String commuting = "Metro card";
			String takeOut = "Uber Eats";
			String grocery = "Grocery";
			
			int commutingCost = 50;
			int takeOutCost = 35;
			int groceryCost = 60;
			int dailyMiscCost = commutingCost + takeOutCost + groceryCost;
			
			
			System.out.println("******** Daily Expenses ********");
			System.out.println();
			
			System.out.println("Subscription Expenses for March 30th, 2022 : ");
			System.out.println();
			System.out.println(subOne + " : " + subOneMonthlyCost/31 + " dollars.");
			System.out.println(subTwo + " : " + subTwoMonthlyCost/31 + " dollars.");
			System.out.println(subThree + " : " + subThreeMonthlyCost/31 + " dollars.");
			System.out.println(subFour + " : " + subFourMonthlyCost/31 + " dollars.");
			System.out.println();
			System.out.print("Total Daily Subscription Expenses for March 30th, 2022 : ");
			System.out.println(dailySubCost + " dollars.");
			System.out.println();
			
			
			System.out.println("Daily Miscellaneous Expenses for March 30th, 2022 : ");
			System.out.println();
			System.out.println(commuting + " : " + commutingCost + " dollars.");
			System.out.println(takeOut + " : " + takeOutCost + " dollars.");
			System.out.println(grocery + " : " + groceryCost + " dollars.");
			System.out.println();
			System.out.print("Total Daily Miscellaneous Expenses for March 30th, 2022 : ");
			System.out.println(dailyMiscCost + " dollars.");
			System.out.println();
			
			System.out.print("Total Daily Expenses for March 30th, 2022 : ");
			System.out.println(dailySubCost + dailyMiscCost + " dollars.");
			
			
			
			
			
			
		}
		
		
		
	}